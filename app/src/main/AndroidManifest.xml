<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.benws.finalproject">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".TopLeft_Activity"
            android:label="@string/title_activity_top_left_"
            android:theme="@style/AppTheme" />
        <activity
            android:name=".TopRight_Activity"
            android:label="@string/title_activity_top_right_"
            android:theme="@style/AppTheme" />
        <activity
            android:name=".BottomLeft_Activity"
            android:label="@string/title_activity_bottom_left_"
            android:theme="@style/AppTheme" />
        <activity
            android:name=".BottomRight_Activity"
            android:label="@string/title_activity_bottom_right_"
            android:theme="@style/AppTheme"></activity>
    </application>

</manifest>